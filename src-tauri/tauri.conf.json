{
  "identifier": "com.lumina.app",
  "productName": "Lumina",
  "version": "0.1.0",
  "build": {
    "beforeDevCommand": "npm run dev:both",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true,
      "capabilities": []
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "Lumina",
        "width": 1200
      }
    ],
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}